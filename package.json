{
  "name": "sabled",
  "version": "0.3.34",
  "description": "A modern, comprehensive React UI component library with beautiful designs and enhanced components. Features advanced Tooltip with smart positioning, Select/Input improvements, Message animations, and more. Built with TailwindCSS and TypeScript for maximum customization and developer experience.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "USAGE.md"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "prepare": "npm run build",
    "pub": "npm version patch && npm publish"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "rollup": "^4.44.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "typescript",
    "ui-library",
